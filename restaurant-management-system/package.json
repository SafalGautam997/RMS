  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
